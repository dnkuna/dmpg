    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/logopro"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Visibility" />
        <activity android:name=".Parents" />
        <activity android:name=".Historique" />
        <activity android:name=".Directive" />
        <activity android:name=".Prescription" />
        <activity android:name=".Antecedent" />
        <activity android:name=".Compte_rendu" />
        <activity android:name=".Analyse" />
        <activity android:name=".Login" />
        <activity android:name=".IntroActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>